#!/usr/bin/make -f

build:

binary: binary-arch binary-indep

binary-arch:

binary-indep:
	dh_prep
	mkdir -p debian/tmp
	cp -a etc debian/tmp/etc
	dh_installdirs --all etc
	dh_install --fail-missing
	dh_gencontrol
	dh_installdeb
	dh_installdocs
	dh_installchangelogs
	dh_md5sums
	dh_builddeb

clean:
	dh_clean
